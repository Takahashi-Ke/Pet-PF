<%= month_calendar(attribute: :created_at, events: memories, previous_link: events_ajax_previous_link, next_link: events_ajax_next_link) do |date, memories| %>
    <%= date.day %>
    <% memories.each do |memory| %>
      <%= link_to memory_path(memory) do %>
        <div class="fas fa-images ml-2"></div>
      <% end %>
    <% end %>
<% end %>